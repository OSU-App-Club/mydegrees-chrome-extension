{
    "manifest_version": 3,
    "name": "Chrome Extension Starter",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "description": "This is the description of your extension",
    "content_scripts": [
        {
            "matches": [
                "https://mydegrees.oregonstate.edu:7447/*",
                "https://mydegrees.oregonstate.edu/*"
            ],
            "js": ["./content.js"]
        },
        {
            "matches": ["https://mydegrees.oregonstate.edu/dashboard/worksheets*"],
            "js": ["./worksheets.js"]
        }
    ],
    "icons": {
        "16": "public/16.png",
        "32": "public/32.png",
        "48": "public/48.png",
        "192": "public/192.png"
    }
}
